{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Evento Parseado",
  "type": "object",
  "properties": {
    "label": { "type": "string", "const": "üóìÔ∏è evento:adicionar" },
    "nome-do-evento": {
      "type": "object",
      "properties": {
        "title": { "type": "string" },
        "content": { "type": "array", "items": { "type": "string" } },
        "text": { "type": "string" }
      },
      "required": ["title", "content", "text"]
    },
    "site-do-evento": {
      "type": "object",
      "properties": {
        "title": { "type": "string" },
        "content": { "type": "array", "items": { "type": "string", "format": "uri" } },
        "text": { "type": "string", "format": "uri" }
      },
      "required": ["title", "content", "text"]
    },
    "formato-do-evento": {
      "type": "object",
      "properties": {
        "title": { "type": "string" },
        "content": { "type": "array", "items": { "type": "string" } },
        "text": { "type": "string", "enum": ["Online", "H√≠brido", "Presencial", "*No response*"] }
      },
      "required": ["title", "content", "text"]
    },
    "cidade-deixe-em-branco-se-online": {
      "type": "object",
      "properties": {
        "title": { "type": "string" },
        "content": { "type": "array", "items": { "type": "string" } },
        "text": { "type": "string" }
      },
      "required": ["title", "content", "text"]
    },
    "estado-provincia-deixe-em-branco-se-online": {
      "type": "object",
      "properties": {
        "title": { "type": "string" },
        "content": { "type": "array", "items": { "type": "string" } },
        "text": { "type": "string" }
      },
      "required": ["title", "content", "text"]
    },
    "pais-deixe-em-branco-se-online": {
      "type": "object",
      "properties": {
        "title": { "type": "string" },
        "content": { "type": "array", "items": { "type": "string" } },
        "text": { "type": "string" }
      },
      "required": ["title", "content", "text"]
    },
    "ano-do-primeiro-dia-de-evento": {
      "type": "object",
      "properties": {
        "title": { "type": "string" },
        "content": { "type": "array", "items": { "type": "string", "pattern": "^[0-9]{4}$" } },
        "text": { "type": "string", "pattern": "^[0-9]{4}$" }
      },
      "required": ["title", "content", "text"]
    },
    "mes-do-primeiro-dia-de-evento": {
      "type": "object",
      "properties": {
        "title": { "type": "string" },
        "content": { "type": "array", "items": { "type": "string" } },
        "text": { "type": "string" }
      },
      "required": ["title", "content", "text"]
    },
    "dia-do-primeiro-dia-de-evento": {
      "type": "object",
      "properties": {
        "title": { "type": "string" },
        "content": { "type": "array", "items": { "type": "string", "pattern": "^[0-9]{1,2}$" } },
        "text": { "type": "string", "pattern": "^[0-9]{1,2}$" }
      },
      "required": ["title", "content", "text"]
    },
    "ano-do-ultimo-dia-de-evento": { "$ref": "#/properties/ano-do-primeiro-dia-de-evento" },
    "mes-do-ultimo-dia-de-evento": { "$ref": "#/properties/mes-do-primeiro-dia-de-evento" },
    "dia-do-ultimo-dia-de-evento": { "$ref": "#/properties/dia-do-primeiro-dia-de-evento" },
    "ano-do-primeiro-dia-de-submissao": { "$ref": "#/properties/ano-do-primeiro-dia-de-evento" },
    "mes-do-primeiro-dia-de-submissao": { "$ref": "#/properties/mes-do-primeiro-dia-de-evento" },
    "dia-do-primeiro-dia-de-submissao": { "$ref": "#/properties/dia-do-primeiro-dia-de-evento" },
    "ano-do-ultimo-dia-de-submissao": { "$ref": "#/properties/ano-do-primeiro-dia-de-evento" },
    "mes-do-ultimo-dia-de-submissao": { "$ref": "#/properties/mes-do-primeiro-dia-de-evento" },
    "dia-do-ultimo-dia-de-submissao": { "$ref": "#/properties/dia-do-primeiro-dia-de-evento" },
    "tipos-de-submissao-aceitos": {
      "type": "object",
      "properties": {
        "title": { "type": "string" },
        "content": { "type": "array" },
        "text": { "type": "string" },
        "list": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "checked": { "type": "boolean" },
              "text": {
                "type": "string",
                "enum": [
                  "Palestra",
                  "Workshop",
                  "Lightning talk",
                  "Painel",
                  "Tutorial",
                  "Minicurso",
                  "Outros (especificar abaixo)"
                ]
              }
            },
            "required": ["checked", "text"]
          }
        }
      },
      "required": ["title", "text", "list"]
    },
    "se-marcou-outros-especifique": {
      "type": "object",
      "properties": {
        "title": { "type": "string" },
        "content": { "type": "array", "items": { "type": "string" } },
        "text": { "type": "string" }
      },
      "required": ["title", "content", "text"]
    }
  },
  "required": [
    "nome-do-evento",
    "site-do-evento",
    "formato-do-evento",
    "ano-do-primeiro-dia-de-evento",
    "mes-do-primeiro-dia-de-evento",
    "dia-do-primeiro-dia-de-evento",
    "ano-do-ultimo-dia-de-evento",
    "mes-do-ultimo-dia-de-evento",
    "dia-do-ultimo-dia-de-evento",
    "ano-do-primeiro-dia-de-submissao",
    "mes-do-primeiro-dia-de-submissao",
    "dia-do-primeiro-dia-de-submissao",
    "ano-do-ultimo-dia-de-submissao",
    "mes-do-ultimo-dia-de-submissao",
    "dia-do-ultimo-dia-de-submissao",
    "tipos-de-submissao-aceitos"
  ]
}
